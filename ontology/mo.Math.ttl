@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs:   <http://www.w3.org/2001/XMLSchema#> .
@prefix mo:   <https://github.com/Mask-coins/mask_documents/ontology/> .


mo:MathExtends
    rdfs:label "拡張"@ja;
    rdfs:comment "数学的な意味での拡張関係を表す。主語が具体・目的語が抽象側。"@ja ;
    a rdf:Property  .

mo:BinaryOperationProperty
    rdfs:label "property"@en;
    a rdf:Property  ;
    a rdfs:Class   ;
    rdfs:comment "二項演算の持つ性質。ClassとPredicateを兼ねる"@ja .

mo:AlgebraicStructure
    rdfs:label "algebraic structure"@en;
    rdfs:label "代数的構造"@ja ;
    a rdfs:Class.

mo:GroupLikeAlgebraicStructure
    rdfs:label "magma"@en;
    rdfs:label "マグマ"@ja ;
    rdfs:subClassOf mo:AlgebraicStructure;
    a rdfs:Class.

mo:RingLikeAlgebraicStructure
    rdfs:label "ring"@en;
    rdfs:label "環"@ja ;
    rdfs:subClassOf mo:AlgebraicStructure;
    a rdfs:Class.





