@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs:   <http://www.w3.org/2001/XMLSchema#> .
@prefix schema:<https://schema.org/> .
@prefix mo:   <https://github.com/Mask-coins/mask_documents/ontology/> .
@prefix mc:   <https://github.com/Mask-coins/mask_documents/category/> .
@prefix mr:   <https://github.com/Mask-coins/mask_documents/resource/> .


mr:BinaryOperation
    rdfs:label "binary operation"@en;
    rdfs:label "二項演算"@ja .

mr:InternalBinaryOperation
    rdfs:label "closed binary operation"@en;
    rdfs:label "internal binary operation"@en;
    rdfs:label "内部二項演算"@ja;
    mo:MathExtends mr:BinaryOperation;
    a rdfs:Class .

mr:ExternallBinaryOperation
    rdfs:label "external binary operation"@en;
    rdfs:label "外部二項演算"@ja;
    mo:MathExtends mr:BinaryOperation .





mr:AssociativeProperty
    rdfs:label "associative property"@en;
    rdfs:label "結合性"@ja;
    a mo:BinaryOperationProperty .

mr:ExistenceOfIdentityElement
    rdfs:label "existence of identity element"@en;
    rdfs:label "単位元が存在"@ja;
    a mo:BinaryOperationProperty .

mr:ExistenceOfInverseElement
    rdfs:label "existence of inverse element"@en;
    rdfs:label "逆元が存在"@ja;
    a mo:BinaryOperationProperty .

mr:CommutativeProperty
    rdfs:label "commutative property"@en;
    rdfs:label "可換性"@ja;
    a mo:BinaryOperationProperty .





mr:AlgebraicStructureMagma
    rdfs:label "magma"@en;
    rdfs:label "マグマ"@ja;
    rdfs:label "groupoid"@en;
    rdfs:label "亜群"@ja;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureUnitalMagma
    rdfs:label "unital magma"@en;
    rdfs:label "単位的マグマ"@ja;
    mo:MathExtends mr:AlgebraicStructureMagma ;
    mo:BinaryOperationProperty mr:ExistenceOfIdentityElement ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureQuasiGroup
    rdfs:label "quasi-group"@en;
    rdfs:label "準群"@ja;
    rdfs:label "quasigroup"@en;
    mo:MathExtends mr:AlgebraicStructureMagma ;
    mo:BinaryOperationProperty mr:ExistenceOfInverseElement ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureLoop
    rdfs:label "loop"@en;
    rdfs:label "擬群"@ja;
    rdfs:label "単位的準群"@ja;
    mo:MathExtends mr:AlgebraicStructureQuasiGroup ;
    mo:MathExtends mr:AlgebraicStructureUnitalMagma ;
    mo:BinaryOperationProperty mr:ExistenceOfIdentityElement ;
    mo:BinaryOperationProperty mr:ExistenceOfInverseElement ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureSemiGroup
    rdfs:label "semigroup"@en;
    rdfs:label "semi-group"@en;
    rdfs:label "半群"@ja;
    mo:MathExtends mr:AlgebraicStructureMagma ;
    mo:BinaryOperationProperty mr:AssociativeProperty ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureInverseSemigroup
    rdfs:label "inverse semigroup"@en;
    mo:MathExtends mr:AlgebraicStructureSemiGroup ;
    mo:MathExtends mr:AlgebraicStructureQuasiGroup ;
    mo:BinaryOperationProperty mr:AssociativeProperty ;
    mo:BinaryOperationProperty mr:ExistenceOfIdentityElement ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureMonoid
    rdfs:label "monoid"@en;
    rdfs:label "モノイド"@ja;
    rdfs:label "単系"@ja;
    rdfs:label "単位的半群"@ja;
    mo:MathExtends mr:AlgebraicStructureSemiGroup ;
    mo:MathExtends mr:AlgebraicStructureUnitalMagma ;
    mo:BinaryOperationProperty mr:AssociativeProperty ;
    mo:BinaryOperationProperty mr:ExistenceOfIdentityElement ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureGroup
    rdfs:label "group"@en;
    rdfs:label "群"@ja;
    mo:MathExtends mr:AlgebraicStructureMonoid ;
    mo:MathExtends mr:AlgebraicStructureInverseSemigroup ;
    mo:MathExtends mr:AlgebraicStructureLoop ;
    mo:BinaryOperationProperty mr:AssociativeProperty ;
    mo:BinaryOperationProperty mr:ExistenceOfIdentityElement ;
    mo:BinaryOperationProperty mr:ExistenceOfInverseElement ;
    a mo:GroupLikeAlgebraicStructure .

mr:AlgebraicStructureAbelianGroup
    rdfs:label "abelian group"@en;
    rdfs:label "アーベル群"@ja;
    rdfs:label "commutative group"@en;
    rdfs:label "可換群"@ja;
    mo:MathExtends mr:AlgebraicStructureGroup ;
    mo:BinaryOperationProperty mr:AssociativeProperty ;
    mo:BinaryOperationProperty mr:ExistenceOfIdentityElement ;
    mo:BinaryOperationProperty mr:ExistenceOfInverseElement ;
    mo:BinaryOperationProperty mr:CommutativeProperty  ;
    a mo:GroupLikeAlgebraicStructure .















